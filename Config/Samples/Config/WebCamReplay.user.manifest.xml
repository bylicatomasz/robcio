<Manifest
    xmlns="http://schemas.microsoft.com/xw/2004/10/manifest.html"
    xmlns:dssp="http://schemas.microsoft.com/xw/2004/10/dssp.html"
    >
  <!-- Starts the webcam replay service -->
  <CreateServiceList>
    <ServiceRecordType>
      <!-- Using the configuration file webcamreplay.user.config.xml -->
      <dssp:Contract>http://schemas.microsoft.com/robotics/2006/07/webcamreplay.user.html</dssp:Contract>
      <dssp:PartnerList>
        <dssp:Partner>
          <dssp:Service>webcamreplay.user.config.xml</dssp:Service>
          <dssp:Name>dssp:StateService</dssp:Name>
        </dssp:Partner>
      </dssp:PartnerList>
    </ServiceRecordType>
  </CreateServiceList>
</Manifest>